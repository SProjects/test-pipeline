<div class= "container content-display">
  <ul id="content" class="flex-tabs tabs">
    <li class="tab"><a href="#curricula" class="active"> Curricula</a></li>
    <li class="tab"><a class="program-tab">Programs</a></li>
  </ul>
    <div id="curricula" class="curricula-display col s12">
      <ul id="content-swipe" class="flex-tabs curricula-tabs tabs">
        <li class="frameworks-tab tab"><a href="#frameworks">Frameworks</a></li>
        <li class="criteria-tab tab"><a id="display_criteria" href="#criteria">Criteria</a></li>
        <li class="output-tab tab"><a class="active" id="display_output" href="#outputs"> Outputs</a></li>
      </ul>
      <div id="outputs">
        <%= render('assessments/assessments')%>
      </div>
      <div id="criteria" class="col s12">
      <!-- Render partials for criteria table and criteria modal -->
        <%= render("criteria/criteria") %>
      </div>
      <div id="frameworks" class="col s12">
        <!-- Render partials for frameworks table -->
          <%= render("frameworks/frameworks") %>
      </div>
    </div>
  </div>
  <%= render("criteria/criterion_modal") %>
  <%= render('assessments/assessment_modal')%>
</div>
